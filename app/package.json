{
  "name": "react-redux-typescript-boilerplate",
  "version": "0.0.1",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "boilerplate",
    "todomvc"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "echo \"Error: no test specified\" && exit 1",
